<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>white.snk</AssemblyOriginatorKeyFile>
    <NoWarn>1591</NoWarn>
    <Copyright>Copyright © TestStack 2012</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>TestStack.White.UIA3.Netstandard</PackageId>
    <RepositoryUrl>https://github.com/zeit200/WhiteNetstandard</RepositoryUrl>
    <Version>1.0.0.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="UIAComWrapperNetstandard" Version="1.0.1" />
    <PackageReference Include="Castle.Core" Version="4.4.1" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="5.0.0" />
    <PackageReference Include="System.IO.FileSystem" Version="4.3.0" />
    <PackageReference Include="System.Drawing.Common" Version="5.0.2" />
  </ItemGroup>
</Project>